{
  "cli": {
    "version": ">= 12.5.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production"
    },
    "release": {
      "distribution": "internal",
      "android": {
        "releaseChannel": "default"
      },
      "channel": "release"
    }
  },
  "submit": {
    "production": {}
  }
}
